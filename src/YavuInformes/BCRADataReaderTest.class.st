{ #category : #'as yet unclassified' }
BCRADataReaderTest >> fvPerson [
	
	^YavuNaturalPerson nombre: 'Facundo Vozzi' dni: '27634975' sexo: 'M'
]

{ #category : #'as yet unclassified' }
BCRADataReaderTest >> test1 [

	| bcraDataReader |
	
	bcraDataReader := (BCRADataReader on: self fvPerson) htmlBasicStream: self fvBCRATestStream.
	bcraDataReader parseHtmlData.
	self assert: bcraDataReader basicData  hasDebtElements.
	self assert: bcraDataReader basicData debtElementsCount = 3.
	self assert: (bcraDataReader basicData existsDebtFor: 'VOZZI FACUNDO') notNil
	

]
